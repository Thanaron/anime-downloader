{
    "name": "anime-downloader",
    "version": "0.7.0",
    "license": "MIT",
    "main": "dist/main-build/index.js",
    "scripts": {
        "start": "cross-env ELECTRON_IS_DEV=0 run-s scripts:clean parcel:build electron:start",
        "dev": "run-p -r parcel:dev electron:dev",
        "build": "run-s  scripts:clean parcel:build \"electron:build {@}\" --",
        "electron:start": "electron dist/main-build/index.js",
        "electron:dev": "nodemon --watch dist/main-build --exec \"electron .\"",
        "electron:build": "electron-builder --dir",
        "parcel:dev": "run-p -r main:dev renderer:dev",
        "parcel:build": "run-s main:build renderer:build",
        "main:dev": "parcel watch src/index.ts --out-dir dist/main-build --target electron",
        "main:build": "parcel build src/index.ts --out-dir dist/main-build --target electron",
        "renderer:dev": "parcel public/index.html --port 3000 --out-dir dist/renderer-build",
        "renderer:build": "parcel build public/index.html --out-dir dist/renderer-build --public-url ./",
        "test": "run-s parcel:build \"test:jest {@}\" --",
        "test:jest": "jest",
        "lint": "tslint app/**/*.ts{,x}",
        "scripts:clean": "ts-node scripts/clean.ts",
        "postinstall": "electron-builder install-app-deps"
    },
    "dependencies": {
        "@ant-design/icons": "^4.2.1",
        "antd": "^4.3.3",
        "api": "^2.0.2",
        "axios": "^0.19.2",
        "electron-is-dev": "^1.2.0",
        "frameless-titlebar": "^2.1.3",
        "nord": "^0.2.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "devDependencies": {
        "@types/react": "^16.9.35",
        "@types/react-dom": "^16.9.8",
        "@types/shelljs": "^0.8.8",
        "cross-env": "^7.0.2",
        "electron": "^9.0.2",
        "electron-builder": "^22.7.0",
        "less": "^3.11.3",
        "parcel": "^1.12.4",
        "shelljs": "^0.8.4",
        "typescript": "^3.9.5"
    },
    "build": {
        "appId": "de.thanaron.anime-downloader",
        "files": [
            "build/**/*"
        ]
    }
}